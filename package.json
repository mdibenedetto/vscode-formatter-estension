{
    "name": "md-tools",
    "displayName": "md-tools",
    "description": "",
    "version": "0.0.1",
    "publisher": "michele",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other",
        "Languages",
        "Formatters"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "MD-Tools.format",
            "title": "MD FORMATTER"
        }],
        "configuration": {
            "type": "object",
            "title": "MD-TOOLS - Beautify Extender config",
            "properties": {
                "beautify.extender": {
                    "type": "object",
                    "commandName": "HookyQR.beautifyFile"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}